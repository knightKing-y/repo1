<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinasofti.dao.tree.TreeDaoInf">

	<select id="getClassRoom" parameterType="com.chinasofti.vo.user.ClassRoom"
		resultType="com.chinasofti.vo.user.ClassRoom">
		select *
		from ClassRoom


	</select>

	<select id="getStudent" parameterType="String"
		resultType="com.chinasofti.vo.user.Student">
		select *
		from Student
		where classid=#{0}


	</select>
	
	
	

</mapper>